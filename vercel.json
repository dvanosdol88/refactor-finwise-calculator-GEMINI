{
    "installCommand":  "npm ci",
    "buildCommand":  "npm run build",
    "outputDirectory":  "dist",
    "framework":  "vite",
    "rewrites":  [
                     {
                         "source":  "/api/(.*)",
                         "destination":  "/api/index.ts"
                     },
                     {
                         "source":  "/alt-calc",
                         "destination":  "/alt-calculator-advisor-value-vs-fees.html"
                     },
                     {
                         "source":  "/silent-killer",
                         "destination":  "/alt-calculator-silent-killer.html"
                     },
                     {
                         "source":  "/((?!api/.*).*)",
                         "destination":  "/index.html"
                     }
                 ]
}